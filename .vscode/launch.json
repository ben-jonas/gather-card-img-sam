{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "AWS_PROFILE": "localstack"
            }
        },
        {
            "name": "All Unit Tests",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}/tests/unit",
                "-v"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src",
                "AWS_PROFILE": "localstack"
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "gather-card-imgs-sam:CardImgAddBatchFunction - valid",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/template.yaml",
                "logicalId": "CardImgAddBatchFunction"
            },
            "lambda": {
                "payload": {
                    "path": "${workspaceFolder}/events/csvupload/happypath/csvupload-valid.json"
                }
            },
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "gather-card-imgs-sam:CardImgViewBatchStatusFunction - nonexistent",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/template.yaml",
                "logicalId": "CardImgViewBatchStatusFunction"
            },
            "lambda": {
                "payload": {
                    "path": "${workspaceFolder}/events/viewstatus/usererror/nonexistent-record.json"
                }
            },
        },
        {
            "type": "debugpy",
            "request": "launch",
            "name": "Invoke cardimg_add_batch lambda handler with Event vs localstack",
            "program": "${workspaceFolder}/localdev/local_runner.py",
            "args": [
                "invoke_handler",
                "${workspaceFolder}/src/cardimg_add_batch/app.py",
                "${workspaceFolder}/events/csvupload/happypath/ten-messages-sample-upload.json"
            ],
            "env": {
                "SCRAPER_APP_VERSION": "local0",
                "APPROVED_DOMAINS_TO_CARDIMG_SELECTORS": "{\"scryfall.com\": \"card\", \"pkmncards.com\": \"card-image\"}",
                "CARDIMG_BUCKET": "gather-card-imgs-sam-card-img-bucket-000000000000",
                "CARD_IMG_FETCH_QUEUE": "http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/CardImgFetchQueue"
            },
            "console": "integratedTerminal"
        },
        {
            "type": "debugpy",
            "request": "launch",
            "name": "Invoke cardimg_single_scrape lambda handler with Event vs localstack",
            "program": "${workspaceFolder}/localdev/local_runner.py",
            "args": [
                "invoke_handler",
                "${workspaceFolder}/src/cardimg_single_scrape/app.py",
                "${workspaceFolder}/events/scraper/happypath/ten-messages-sample.json"
            ],
            "env": {
                "SCRAPER_APP_VERSION": "local0",
                "APPROVED_DOMAINS_TO_CARDIMG_SELECTORS": "{\"scryfall.com\": \"card\", \"pkmncards.com\": \"card-image\"}",
                "CARDIMG_BUCKET": "gather-card-imgs-sam-card-img-bucket-000000000000"
            },
            "console": "integratedTerminal"
        },
        {
            "type": "debugpy",
            "request": "launch",
            "name": "Load new batch-in-progress from csv file to localstack's dynamodb",
            "program": "${workspaceFolder}/localdev/local_runner.py",
            "args": [
                "create_batch_in_progress",
                "${workspaceFolder}",
                "${workspaceFolder}/events/csvupload/happypath/ten-messages-sample-upload.json",
                "7bbf9329-bad0-4be6-89e1-0490b99330d6"
            ],
            "env": {
                "SCRAPER_APP_VERSION": "local0",
                "APPROVED_DOMAINS_TO_CARDIMG_SELECTORS": "{\"scryfall.com\": \"card\", \"pkmncards.com\": \"card-image\"}",
                "CARDIMG_BUCKET": "gather-card-imgs-sam-card-img-bucket-000000000000"
            },
            "console": "integratedTerminal"
        }
    ]
}